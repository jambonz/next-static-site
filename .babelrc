{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "js",
          "json",
          "bash",
          "http",
          "html",
          "css",
          "jsx",
          "sass",
          "scss"
        ],
        "plugins": [
          "line-numbers",
          "show-language"
        ],
        "theme": "okaidia",
        "css": true
      }
    ]
  ]
}